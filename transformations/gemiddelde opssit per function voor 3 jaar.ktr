    <connection>localhost</connection>
    <sql>SELECT&#xd;&#xa;mrqr_assets.function&#xd;&#xa;, date_field&#xd;&#xa;, COUNT&#x28;mrqr_assets.ASSET&#x29; AS opscount&#xd;&#xa;FROM mrqr_assets&#xd;&#xa;inner join daotg4_functions_global on mrqr_assets.nsn&#x3d;daotg4_functions_global.nsn and mrqr_assets.function&#x3d;daotg4_functions_global.function &#xd;&#xa;INNER JOIN MRQR_ASSETS_OPERATIONAL_STATUS_LAST_C ON mrqr_assets.ASSET&#x3d;MRQR_ASSETS_OPERATIONAL_STATUS_LAST_C.ASSET&#xd;&#xa;INNER JOIN mrqr_aosac_intervals ON mrqr_assets.ASSET&#x3d;mrqr_aosac_intervals.ASSET &#xd;&#xa;,damrsysl_calendar_dates&#xd;&#xa;&#xd;&#xa;WHERE daotg4_functions_global.top&#x3d;1&#xd;&#xa;AND mrqr_assets.STATUS NOT IN&#x28;&#x27;D&#x27;,&#x27;A&#x27;,&#x27;B&#x27;,&#x27;E&#x27;,&#x27;O&#x27;&#x29; AND &#x28;mrqr_assets.STORE NOT LIKE&#x28;&#x27;&#x25;VE&#x27;&#x29; OR mrqr_assets.STORE IS NULL&#x29;&#xd;&#xa;AND mrqr_aosac_intervals.STATUS IN &#x28;&#x27;O&#x27;,&#x27;Q&#x27;&#x29;&#xd;&#xa;AND ADDTIME&#x28;date_field, &#x27;09&#x3a;00&#x3a;00.0&#x27;&#x29; &#x3e;&#x3d; STARTDATE AND ADDTIME&#x28;date_field, &#x27;09&#x3a;00&#x3a;00.0&#x27;&#x29; &#x3c; ENDDATE&#xd;&#xa;AND date_field &#x3e;&#x3d; DATE_SUB&#x28;CURRENT_DATE, INTERVAL 3 YEAR&#x29; AND date_field &#x3c; CURRENT_DATE&#xd;&#xa;and &#x28;mrqr_assets.function in&#x28;&#x27;AI3D&#x27;,&#x27;AI9D&#x27;,&#x27;AIAM&#x27;,&#x27;AIFU&#x27;,&#x27;AIGN&#x27;,&#x27;AIPC&#x27;,&#x27;AIRC&#x27;,&#x27;VDAM&#x27;,&#x27;VDFF&#x27;,&#x27;VDFU&#x27;,&#x27;VDPC&#x27;,&#x27;VDSC&#x27;,&#x27;IVLM&#x27;,&#x27;IVLM-SPS&#x27;,&#x27;RBPAM&#x27;,&#x27;RBPBG&#x27;,&#x27;RBPRE&#x27;,&#x27;IV8T&#x27;,&#x27;IV12T&#x27;,&#x27;YT43F1AVO&#x27;,&#x27;YT43FKGVO&#x27;,&#x27;YT43FVAVO&#x27;,&#x27;YT43FWAVO&#x27;&#x29;&#xd;&#xa;or mrqr_assets.function like &#x27;UG&#x25;&#x27;&#x29;&#xd;&#xa;GROUP BY function,  date_field</sql>
